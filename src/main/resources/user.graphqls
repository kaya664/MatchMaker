type Query {
    users: [User]
}

type User {
    id: ID!
    name: String
    userName: String
    password: String
}

type AuthenticationRepsonse {
    token: String
}

type Mutation {
    newUser(name: String!, userName: String!, password: String!) : User!
    authenticate(userName: String, password: String) : AuthenticationRepsonse!
}