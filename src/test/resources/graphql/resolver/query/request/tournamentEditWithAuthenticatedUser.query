mutation {
  editTournament(tournamentEditRequest: {tournament: {id: "tournamentId", name: "FIFA", gameName: "FIFA", paymentType: "Havale", paymentCurrency: "TL", eliminationType: "1vs1", status: "New", explanation: "csgo match"}}) {
    editedTournament {
      name
      gameName
      paymentType
      paymentCurrency
      eliminationType
      status
      explanation
    }
  }
}